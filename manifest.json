{
  "manifest_version": 2,
  "name": "Tabuji",
  "version": "1.0.0",
  "icons": { 
    "16": "/assets/icon16.png",
    "48": "/assets/icon48.png",
   "128": "/assets/icon128.png" },
  "description": "A random chinese word every time you open a new tab",
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "https://www.purpleculture.net/"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}